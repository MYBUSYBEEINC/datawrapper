{
    "name": "@datawrapper/chart-core",
    "version": "8.47.0",
    "description": "Svelte component to render charts.",
    "main": "index.js",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=7.0.0"
    },
    "files": [
        "dist",
        "lib"
    ],
    "scripts": {
        "format": "cd ../.. && scripts/format libs/chart-core",
        "build": "rollup -c",
        "dev": "rollup -cw",
        "lint": "cd ../.. && scripts/lint libs/chart-core",
        "prepublishOnly": "npm run build",
        "test": "ava",
        "docs:parser": "jsdoc2md  --template docs/.tpl.hbs --files 'lib/dw/utils/parser.js' -g grouped  | sed '/\\*\\*Kind\\*\\*/d'  | sed '/\\*\\*Example\\*\\*/d' | sed '/## $/d' | sed 's/## \\([a-z]\\)/### \\1/' > docs/parser.md && node docs/.fix.js parser.md"
    },
    "dependencies": {
        "@datawrapper/expr-eval": "^2.0.5",
        "@datawrapper/polyfills": "file:../polyfills",
        "@datawrapper/shared": "^0.49.2",
        "@emotion/css": "^11.1.3",
        "autoprefixer": "^10.4.11",
        "core-js": "3.6.5",
        "cssnano": "^5.1.13",
        "deepmerge": "^4.2.2",
        "fontfaceobserver": "2.1.0",
        "less": "^4.1.3",
        "numeral": "^2.0.6",
        "postcss": "^8.4.16",
        "postcss-default-unit": "^1.0.0",
        "postcss-less": "^6.0.0",
        "svelte-extras": "^2.0.2",
        "svelte2": "npm:svelte@^2.16.1",
        "underscore": "^1.13.1"
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@babel/plugin-transform-runtime": "7.9.0",
        "@babel/preset-env": "7.9.5",
        "@babel/runtime": "7.9.2",
        "@datawrapper/service-utils": "file:../service-utils",
        "@rollup/plugin-alias": "^3.1.2",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^3.0.1",
        "ava": "^3.8.1",
        "babel-plugin-transform-async-to-promises": "0.8.15",
        "d3-jetpack": "^2.0.19",
        "d3-selection": "^1.3.0",
        "jsdoc-to-markdown": "^5.0.0",
        "lint-staged": "^12.1.2",
        "puppeteer": "^17.1.3",
        "rollup": "^2.64.0",
        "rollup-plugin-babel": "4.4.0",
        "rollup-plugin-svelte": "5.2.1",
        "rollup-plugin-terser": "^7.0.0",
        "sinon": "^11.1.1",
        "svelte": "3.23.2"
    },
    "ava": {
        "nodeArguments": [
            "--experimental-specifier-resolution=node",
            "--no-warnings"
        ]
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    }
}
