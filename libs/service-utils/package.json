{
    "name": "@datawrapper/service-utils",
    "version": "1.5.1",
    "description": "Shared functions used throughout Datawrapper's server-side node components",
    "author": "Datawrapper GmbH",
    "type": "commonjs",
    "main": "dist/index.js",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=7.0.0"
    },
    "scripts": {
        "test": "npm run build && ava",
        "test:ci": "ava",
        "test:watch": "ava -w",
        "build": "rm -Rf dist/* && tsc",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run lint && npm run build && npm test && rm dist/*.test.*",
        "lint": "cd ../.. && scripts/lint libs/service-utils",
        "format": "cd ../.. && scripts/format libs/service-utils"
    },
    "dependencies": {
        "@datawrapper/locales": "^1.2.13",
        "@datawrapper/orm": "file:../orm",
        "@hapi/boom": "^9.1.0",
        "assign-deep": "^1.0.1",
        "bcryptjs": "^2.4.3",
        "fast-glob": "^3.2.11",
        "fs-extra": "^10.0.0",
        "humps": "^2.0.1",
        "lodash": "^4.17.20",
        "nanoid": "^3.1.22"
    },
    "devDependencies": {
        "@datawrapper/backend-utils": "file:../backend-utils",
        "@datawrapper/shared": "^1.5.3",
        "@tsconfig/node18-strictest": "^1.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/fs-extra": "^9.0.13",
        "@types/hapi": "^18.0.7",
        "@types/humps": "^2.0.2",
        "@types/lodash": "^4.14.188",
        "@types/pino": "^7.0.5",
        "@types/sinon": "^10.0.13",
        "@types/tmp": "^0.2.3",
        "ava": "^5.0.1",
        "browser-env": "^3.3.0",
        "bullmq": "^1.91.1",
        "sinon": "^14.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
