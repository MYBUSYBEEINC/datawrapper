{
    "name": "d3-maps-build",
    "version": "1.0.0",
    "scripts": {
        "format": "prettier 'src/**/*.{js,html}' --write",
        "lint": "prettier --check 'src/**/*.{js,html}' && healthier 'src/**/*.{js,html}'",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.0",
        "@mapbox/vector-tile": "^1.3.1",
        "@turf/area": "^6.3.0",
        "@turf/bbox": "^6.3.0",
        "@turf/bbox-polygon": "^6.3.0",
        "@turf/center": "^6.3.0",
        "d3-array": "^1.2.1",
        "d3-collection": "^1.0.4",
        "d3-color": "^1.4.1",
        "d3-format": "^1.4.5",
        "d3-geo": "^1.12.1",
        "d3-geo-projection": "^2.9.0",
        "d3-geo-voronoi": "^1.6.0",
        "d3-jetpack": "2.0.19",
        "d3-scale": "^2.2.2",
        "d3-selection": "^1.4.2",
        "d3-tile": "^1.0.0",
        "d3-transition": "^1.3.2",
        "d3-voronoi": "^1.1.4",
        "d3-zoom": "^1.8.3",
        "deepmerge": "^4.2.2",
        "geo-albers-usa-territories": "^0.1.0",
        "got": "^10.7.0",
        "lodash": "^4.17.20",
        "mapshaper": "^0.5.30",
        "node-fetch": "^2.6.1",
        "numeral": "^2.0.6",
        "pbf": "^3.2.1",
        "topojson": "^3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.10.0",
        "@babel/plugin-transform-runtime": "^7.10.0",
        "@babel/preset-env": "^7.8.4",
        "@datawrapper/block-annotations": "^1.0.1",
        "@datawrapper/block-color-legend": "^1.0.2",
        "@datawrapper/block-color-scale": "^1.0.4",
        "@datawrapper/chart-core": "^8.21.0",
        "@datawrapper/controls": "^2.34.0",
        "@datawrapper/eslint-config": "^0.1.0",
        "@datawrapper/shared": "^0.31.2",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.0.2",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-commonjs": "^15.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@tivac/eslint-plugin-svelte": "^2.0.0",
        "@types/babel__core": "^7.1.9",
        "ava": "^2.4.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-transform-async-to-promises": "^0.8.15",
        "d3-hexbin": "^0.2.2",
        "d3-polygon": "^1.0.6",
        "d3-quadtree": "^1.0.7",
        "dayjs": "^1.8.13",
        "eslint": "^7.21.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-html": "^6.1.1",
        "healthier": "^4.0.0",
        "husky": "^4.2.5",
        "lint-staged": "^8.2.1",
        "lodash-es": "^4.17.15",
        "prettier": "^1.19.1",
        "rollup": "^2.27.0",
        "rollup-plugin-svelte": "^5.2.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-watch": "^4.3.1",
        "svelte": "^2.16.1",
        "svelte-transitions": "^1.2.0",
        "svelte2": "npm:svelte@^2.16.1",
        "topojson-client": "^3.0.0",
        "topojson-simplify": "^3.0.3",
        "underscore": "^1.9.1"
    },
    "peerDependencies": {
        "joi": "17.x"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && git rev-parse HEAD > .githead && git add .githead"
        }
    },
    "lint-staged": {
        "linters": {
            "*.{js,html}": [
                "prettier --write",
                "healthier",
                "git add"
            ]
        },
        "ignore": [
            "static/*"
        ]
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": [
            "@datawrapper/eslint-config/chart-plugin",
            "plugin:@tivac/svelte/svelte"
        ],
        "plugins": [
            "html",
            "@tivac/svelte"
        ],
        "rules": {
            "@tivac/svelte/property-ordering": "off",
            "@tivac/svelte/onupdate": "off"
        }
    },
    "prettier": {
        "arrowParens": "avoid",
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none"
    },
    "ava": {
        "require": [
            "esm"
        ],
        "files": [
            "./src/*.test.js"
        ],
        "sources": [
            "./*.{js,svelte}"
        ]
    }
}
