{
  "name": "d3-pies",
  "version": "1.0.5",
  "scripts": {
    "format": "prettier src/**/*.js --write && eslint src --fix",
    "lint": "eslint src",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "watch-css": "chokidar ./src/styles/d3-pies.src.css -c \"npm run process-css\"",
    "process-css": "postcss ./src/styles/d3-pies.src.css --no-map --use autoprefixer -o ./static/d3-pies.css"
  },
  "prettier": {
    "tabWidth": 4,
    "semi": true,
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "@tivac/eslint-plugin-svelte": "^1.2.1",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "chokidar-cli": "^1.2.1",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "postcss-cli": "^6.1.1",
    "prettier": "^1.16.4",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-watch": "^4.3.1",
    "svelte": "^2.16.0"
  },
  "dependencies": {
    "d3-color": "^1.2.3",
    "d3-jetpack": "^2.0.19",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "numeral": "^2.0.6",
    "underscore-es": "^1.9.8"
  }
}
