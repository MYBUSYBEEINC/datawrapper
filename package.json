{
    "name": "@datawrapper/plugin-visualization-column-charts",
    "version": "1.6.3",
    "scripts": {
        "format": "prettier 'src/**/*.{js,html}' --write",
        "lint": "prettier --check 'src/**/*.{js,html}' && healthier 'src/**/*.{js,html}'",
        "build": "rollup -c",
        "dev": "rollup -c -w"
    },
    "dependencies": {
        "@datawrapper/chart-core": "^8.18.9",
        "@datawrapper/raphael-chart": "^1.0.2",
        "deepmerge": "^4.2.2",
        "numeral": "^2.0.6",
        "underscore-es": "^1.9.8"
    },
    "devDependencies": {
        "@datawrapper/block-annotations": "^2.0.3",
        "@datawrapper/block-get-color": "^1.0.1",
        "@datawrapper/block-xy-grid": "^2.1.0",
        "@datawrapper/controls": "^2.34.6",
        "@datawrapper/eslint-config": "^0.1.1",
        "@datawrapper/prettier-config": "^1.0.0",
        "@datawrapper/shared": "^0.30.3",
        "@rollup/plugin-replace": "^2.3.4",
        "@tivac/eslint-plugin-svelte": "^1.3.0",
        "babel-eslint": "^10.0.1",
        "chroma-js": "^2.0.6",
        "d3-array": "^1.2.4",
        "d3-jetpack": "^2.0.19",
        "d3-scale": "^2.2.2",
        "d3-selection": "^1.3.0",
        "d3-shape": "^1.3.5",
        "d3-time-format": "^2.2.3",
        "dayjs": "^1.8.17",
        "eslint": "^7.24.0",
        "eslint-plugin-html": "^6.1.2",
        "healthier": "^4.0.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.0",
        "prettier": "^1.19.1",
        "rollup": "^1.17.0",
        "rollup-plugin-buble": "^0.19.6",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-livereload": "^1.0.1",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-svelte": "^5.1.0",
        "rollup-plugin-terser": "^5.1.1",
        "rollup-watch": "^3.2.2",
        "svelte": "^2.16.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "linters": {
            "*.{js,html}": [
                "prettier --write",
                "healthier",
                "git add"
            ]
        },
        "ignore": [
            "static/**/*.js",
            "src/render/shared/symbolPolyfill.js"
        ]
    },
    "prettier": "@datawrapper/prettier-config",
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": [
            "@datawrapper/eslint-config/chart-plugin",
            "plugin:@tivac/svelte/svelte"
        ],
        "plugins": [
            "html",
            "@tivac/svelte"
        ]
    }
}
