{
    "name": "export-zip",
    "version": "1.0.0",
    "description": "",
    "main": "api.js",
    "scripts": {
        "format": "prettier 'src/**/*.js' --write",
        "lint": "prettier --check 'src/**/*.js' && healthier 'src/**/*.js'",
        "build": "rollup -c",
        "dev": "rollup -cw"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@datawrapper/service-utils": "^1.1.0",
        "archiver": "4.0.1",
        "got": "^11.1.0",
        "svelte": "^2.16.1"
    },
    "devDependencies": {
        "@datawrapper/shared": "^0.27.6",
        "downloadjs": "^1.4.7",
        "healthier": "3.3.0",
        "husky": "4.2.5",
        "lint-staged": "10.1.7",
        "prettier": "2.0.5",
        "rollup": "^2.7.2",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-svelte": "^5.2.1",
        "rollup-plugin-uglify": "^6.0.4"
    },
    "lint-staged": {
        "src/**/*.js": [
            "prettier --write",
            "healthier"
        ]
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "rules": {
            "no-console": [
                "error",
                {
                    "allow": [
                        "warn",
                        "error"
                    ]
                }
            ],
            "camelcase": [
                "warn",
                {
                    "ignoreDestructuring": true,
                    "properties": "never"
                }
            ]
        }
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "printWidth": 100,
        "singleQuote": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
